            function CloseIcon(AbjIcon, j) {
                icon = document.getElementById(AbjIcon);
                for (i = 1; i <= j; i++) {
                    kk = document.getElementById("topmu" + i);
                    if (kk != null) {
                        if (icon == kk.id) {
