    <script type="text/javascript">
            $(function() {
                var currSlide;
                $('.slideshow')
                    .before('<div class="homeimage"><div id="homeimage">')
                    .cycle({
                        speed           : 1500,
                        fx              : 'uncover,uncover,uncover,uncover',
                        easing          : 'easeInBack',
                        randomizeEffects: false,
                        pause           : 1,
                        timeoutFn       : function (curr, next, opts, fwd) {
                            return [2, 2, 2, 2][opts.currSlide] * 1000;
                        },
                        pager           : '#homeimage',
                        slideResize     : 0,
                        after           : function (curr, next, opts, fwd) {
                            currSlide = $(this);
                            trackSlideHeight();
                        }
                    });
                
                $(window).on('resize', function () {
                    var currSlideHeight = 0;
                    if (currSlide) {
                        currSlideHeight = currSlide.height();
                    }
                    $('.slideshow').css({ height: currSlideHeight, width: '100%' });
                });
                
                function trackSlideHeight() {
                    if (currSlide && currSlide.height()) {
                        $('.slideshow').animate({ height: currSlide.height() });
                    } else {
                        setTimeout(trackSlideHeight, 100);
                    }
                }
            });
        </script>

